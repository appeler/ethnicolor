citHeader("To cite package 'ethnicolor' in publications use:")
     
year <- sub(".*(2[[:digit:]]{3})-.*", "\\1", meta$Date, perl = TRUE)
vers <- paste("R package version", meta$Version)
 
citEntry(entry="Manual",
		  title = "ethnicolor: Impute Race and Ethnicity Based on Name",
		  author = personList(as.person("Gaurav SOod")),
		  year = year,
		  note = vers,
		  textVersion =
		  paste("Gaurav Sood (",
				year,
				"). ethnicolor: Impute Race and Ethnicity Based on Name. ",
				vers, ".", sep=""))